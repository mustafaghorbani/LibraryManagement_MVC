@model IEnumerable<DailyReportModel>
@{
    ViewData["Title"] = "Home Page";
}

<div class="col-md-12 page-header" >
    Daily Report:
</div>
<table class="table">
    <thead>
        <tr>
            <th scope="col">@Html.DisplayNameFor(model => model.BookTitle)</th>
            <th scope="col">@Html.DisplayNameFor(model => model.ISBN)</th>
            <th scope="col">@Html.DisplayNameFor(model => model.MemberId)</th>
            <th scope="col">@Html.DisplayNameFor(model => model.MemberName)</th>
            <th scope="col">@Html.DisplayNameFor(model => model.DueDate)</th>
            <th scope="col">@Html.DisplayNameFor(model => model.IsLateDueDate)</th>
            <th scope="col">@Html.DisplayNameFor(model => model.Penalty)</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td scope="row">
                    @Html.DisplayFor(modelItem => item.BookTitle)
                </td>
                <td scope="row">
                    @Html.DisplayFor(modelItem => item.ISBN)
                </td>
                <td scope="row">
                    @Html.DisplayFor(modelItem => item.MemberId)
                </td>
                <td scope="row">
                    @Html.DisplayFor(modelItem => item.MemberName)
                </td>
                <td scope="row">
                    @Html.DisplayFor(modelItem => item.DueDate)
                </td>
                <td scope="row">
                    @Html.DisplayFor(modelItem => item.IsLateDueDate)
                </td>
                <td scope="row">
                    @Html.DisplayFor(modelItem => item.Penalty)
                </td>
            </tr>
        }
    </tbody>
</table>
